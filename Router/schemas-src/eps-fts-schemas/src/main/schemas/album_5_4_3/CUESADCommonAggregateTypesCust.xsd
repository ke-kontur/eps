<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:catESAD_cu="urn:customs.ru:CUESADCommonAggregateTypesCust:5.4.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.4.0" xmlns:cltESAD_cu="urn:customs.ru:CUESADCommonLeafTypes:5.4.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:customs.ru:CUESADCommonAggregateTypesCust:5.4.0" elementFormDefault="qualified" version="5.4.0.0">
	<xs:import namespace="urn:customs.ru:CUESADCommonLeafTypes:5.4.0" schemaLocation="CUESADCommonLeafTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CommonLeafTypes:5.4.0" schemaLocation="CommonLeafTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.4.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
	<xs:annotation>
		<xs:documentation>Таможенные документы. Общие сложные типы</xs:documentation>
	</xs:annotation>
	<xs:complexType name="AutomobileType">
		<xs:annotation>
			<xs:documentation>Сведения об автомобилях. Гр. 31</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:AutomobileBaseType">
				<xs:sequence>
					<xs:element name="IdentityCardNumber" type="clt_ru:DocumentNumberType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Номер удостоверения</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CarCost" type="clt_ru:AmountType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Стоимость транспортного средства</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PowerWeightQuanity" type="clt_ru:EnginePowerQuanityType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Примечание: грузоподъемность/мощность</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PassedKilometerQuantity" type="clt_ru:Quantity8Type" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Пробег</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CUConsigmentType">
		<xs:annotation>
			<xs:documentation>Сведения о перевозке товаров. Гр. 15, 15а, 17,17а, 18, 19, 21, 25, 26, 29  -ДТ/ Гр. 15, 17, 18, 19, 21, 25, 26, 29 , 53, 55- ТД</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ContainerIndicator" type="clt_ru:IndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак контейнерных перевозок. Гр.19 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DispatchCountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Страна отправления. Буквенный код по  классификатору стран мира .гр 15, подраздел a </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RBDispatchCountryCode" type="clt_ru:Code3Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код административно-территориального деления страны отправления в соответствии с классификатором административно-территориального деления стран, применяемым в Республике Беларусь. Гр.15,  подраздел b</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DispatchCountryName" type="clt_ru:CountryNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Страна отправления. Краткое название Гр.15</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationCountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Страна назначения. Буквенный код по классификатору стран мира / 00 (неизвестна). Гр 17, подраздел a</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationCountryName" type="clt_ru:CountryNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Страна назначения. Гр.17 Название/ НЕИЗВЕСТНА</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RBDestinationCountryCode" type="clt_ru:Code3Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код административно-территориального деления страны назначения в соответствии с классификатором административно-территориального деления стран, применяемым в Республике Беларусь. Гр.17,  подраздел b</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BorderCustomsOffice" type="cat_ru:CUCustomsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Таможенный орган въезда/выезда. Гр.29</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CUCustomsPaymentCalculationType">
		<xs:annotation>
			<xs:documentation>Исчисление таможенных платежей</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PaymentModeCode" type="clt_ru:CUCustomsPaymentCodeType">
				<xs:annotation>
					<xs:documentation>Код вида платежа  по классификатору видов налогов, сборов и иных платежей, взимание которых возложено на таможенные органы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentAmount" type="clt_ru:AmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сумма платежа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentCurrencyCode" type="clt_ru:CurrencyN3CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Цифровой код валюты суммы платежа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxBase" type="clt_ru:QuantityBasisType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Основа начисления</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxBaseCurrencyCode" type="clt_ru:CurrencyN3CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Цифровой код валюты основы начисления (адвалорная ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Rate" type="cltESAD_cu:RateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ставка / ставка рефинансирования</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateTypeCode" type="clt_ru:LetterIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Вид ставки (адвалорная -"%", признак сборов для РК - "S", специфическая - "*")</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateCurrencyCode" type="clt_ru:CurrencyN3CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Цифровой код валюты ставки (специфическая ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateTNVEDQualifierCode" type="clt_ru:MeasureUnitQualifierCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код единицы измерения ставки (специфическая ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightingFactor" type="cltESAD_cu:WeightingFactorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Весовой коэффициент единицы измерения (специфическая ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ComparisonOperationsSign" type="clt_ru:Numeric1Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Знак операции сравнения (1 - меньше, 2 - больше)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Rate2" type="cltESAD_cu:RateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ставка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateTypeCode2" type="clt_ru:LetterIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Вид ставки (адвалорная -"%", признак сборов для РК - "S", специфическая - "*")</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateCurrencyCode2" type="clt_ru:CurrencyN3CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Цифровой код валюты ставки (специфическая ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateTNVEDQualifierCode2" type="clt_ru:MeasureUnitQualifierCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код единицы измерения ставки (специфическая ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightingFactor2" type="cltESAD_cu:WeightingFactorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Весовой коэффициент единицы измерения (специфическая ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OperationsSign" type="clt_ru:LetterIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Знак операции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Rate3" type="cltESAD_cu:RateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ставка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateTypeCode3" type="clt_ru:LetterIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Вид ставки (адвалорная -"%", специфическая - "*")</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateCurrencyCode3" type="clt_ru:CurrencyN3CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Цифровой код валюты ставки (специфическая ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateTNVEDQualifierCode3" type="clt_ru:MeasureUnitQualifierCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код единицы измерения ставки (специфическая ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightingFactor3" type="cltESAD_cu:WeightingFactorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Весовой коэффициент единицы измерения (специфическая ставка)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ComparisonResult" type="clt_ru:IndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Результат выполнения операции сравнения: 1 - истина; 0 - ложь</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateUseDate" type="clt_ru:DateCustType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата применения ставки таможенного платежа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentCode" type="clt_ru:Code2Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код в соответствии с классификатором особенностей уплаты таможенных и иных платежей, взимание которых возложено на таможенные органы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumberDays" type="clt_ru:Count4Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Количество дней</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumberStages" type="clt_ru:Count3Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Число этапов</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumberMonths" type="clt_ru:Count3Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Число полных и неполных календарных месяцев</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TariffRate" type="clt_ru:Quantity4point2Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Тарифный коэффициент</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineNumber" type="clt_ru:SheetNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Порядковый номер строки из гр.40</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumberGroup" type="clt_ru:Text50Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер группы. Для РФ</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CUDelayPaymentsType">
		<xs:annotation>
			<xs:documentation>Отсрочка платежей гр.48</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PaymentModeCode" type="clt_ru:CustomsPaymentCodeType">
				<xs:annotation>
					<xs:documentation>Код вида таможенного платежа по классификатору видов налогов, сборов и иных платежей, взимание которых возложено на таможенные органы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DelayDocumentNumber" type="clt_ru:DocumentNumberType">
				<xs:annotation>
					<xs:documentation>Номер документа, на основании которого предоставлена отсрочка </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DelayDocumentDate" type="clt_ru:DateCustType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата документа, на основании которого предоставлена отсрочка </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DelayDate" type="clt_ru:DateCustType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата уплаты</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CUESADCustomsRepresCertificateType">
		<xs:annotation>
			<xs:documentation>Документ, свидетельствующий о включении лица  в Реестр таможенных представителей</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:DocumentBaseType">
				<xs:sequence>
					<xs:element name="DocumentModeCode" type="clt_ru:LetterIndicatorType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Тип документа:2 - свидетельство таможенного  представителя (брокера); 3 - свидетельство таможенного представителя</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CUESADCustomsRepresentativeType">
		<xs:annotation>
			<xs:documentation>Таможенный представитель</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ContractRepresDecl" type="cat_ru:DocumentBaseType">
				<xs:annotation>
					<xs:documentation>Договор таможенного представителя с декларантом</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomsRepresCertificate" type="catESAD_cu:CUESADCustomsRepresCertificateType">
				<xs:annotation>
					<xs:documentation>Документ, свидетельствующий о включении лица  в Реестр таможенных представителей</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:element name="RFOrganizationFeatures" type="cat_ru:RFOrganizationFeaturesType">
					<xs:annotation>
						<xs:documentation>Сведения об организации. Особенности Российской Федерации</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RKOrganizationFeatures" type="cat_ru:RKOrganizationFeaturesType">
					<xs:annotation>
						<xs:documentation>Сведения об организации. Особенности Республики Казахстан</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RBOrganizationFeatures" type="cat_ru:RBOrganizationFeaturesType">
					<xs:annotation>
						<xs:documentation>Сведения об организации. Особенности Республики Беларусь</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CUESADGoodsItemType">
		<xs:annotation>
			<xs:documentation>Сведения о товаре</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="catESAD_cu:GoodsItemBaseType">
				<xs:sequence>
					<xs:element name="GoodsTNVEDCode" type="clt_ru:CUGoodsNomenclatuerCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Код товара по ТН ВЭД ТС. Гр.33 первый раздел</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GoodsClassificationCode" type="clt_ru:LetterIndicatorType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Особенность классификации кода: 1 - общий "О", 2 - товар списка. В остальных случаях не заполняется</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AdditionalSign" type="clt_ru:LetterIndicatorType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Дополнительный признак нетарифного регулирования ("С"). Гр.33 второй раздел</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IntellectPropertySign" type="clt_ru:LetterIndicatorType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Признак объекта интеллектуальной собственности ("И"). Гр.33 второй раздел  в ДТ</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RestrictionSign" type="clt_ru:LetterIndicatorType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Буквенный код соблюдения запретов и ограничений в соответствии с классификатором. Для РК</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BeginPeriodDate" type="clt_ru:DateCustType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Дата начала периода (ОПД)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndPeriodDate" type="clt_ru:DateCustType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Дата окончания периода (ОПД)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OriginCountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Код страны происхождения по классификатору стран мира / "EU" / 00 (неизвестна)  Гр.34</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OriginCountryName" type="clt_ru:CountryNameType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Краткое название страны происхождения / экономического союза</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CustomsCostCorrectMethod" type="cltESAD_cu:CustomsCostAppraiseMethodType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Код метода определения таможенной стоимости. В соответствии с классификатором методов определения таможенной стоимости. Гр. 43</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GoodsAddTNVEDCode" type="clt_ru:GoodsNomenclatuerAddCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Код  товара в соответствии с классификатором дополнительной таможенной информации</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DeliveryTime" type="clt_ru:DateCustType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Дата начала периода поставки. (элемент 7 гр. 31 ДТ)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DeliveryTimeEND" type="clt_ru:DateCustType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Дата окончания периода поставки. (элемент 7 гр. 31 ДТ)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AdditionalSheetCount" type="clt_ru:SheetNumberType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Порядковый номер листа (первый подраздел гр.3)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="QuantityFact" type="clt_ru:QuantityBasisType" minOccurs="0">
						<xs:annotation>
