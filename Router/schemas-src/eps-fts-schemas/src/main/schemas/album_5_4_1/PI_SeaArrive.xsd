               </xs:element>
               <xs:element name="CrewInfo" type="pisa:CrewInfoType">
                  <xs:annotation>
                     <xs:documentation>Сведения об экипаже</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" name="Consignments" type="pisa:ConsignmentsType">
                  <xs:annotation>
                     <xs:documentation>Перечень товарных партий</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Stores" type="pisa:StoresType">
                  <xs:annotation>
                     <xs:documentation>Сведения о судовых припасах</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
            <xs:attribute fixed="1006816E" name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required">
               <xs:annotation>
                  <xs:documentation>Идентификатор вида документа</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:annotation>
      <xs:documentation>Локальные прикладные типы</xs:documentation>
   </xs:annotation>
