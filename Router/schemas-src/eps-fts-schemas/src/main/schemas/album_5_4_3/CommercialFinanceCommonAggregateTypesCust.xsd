<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="urn:customs.ru:Information:CommercialFinanceDocuments:CommercialFinanceCommonAgregateTypesCust:5.4.0" version="5.4.0.0" xmlns:catComFin_ru="urn:customs.ru:Information:CommercialFinanceDocuments:CommercialFinanceCommonAgregateTypesCust:5.4.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.4.0" xmlns:cltComFin_ru="urn:customs.ru:Information:CommercialFinanceDocuments:CommercialFinanceCommonLeafTypesCust:5.1.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:import namespace="urn:customs.ru:Information:CommercialFinanceDocuments:CommercialFinanceCommonLeafTypesCust:5.1.0" schemaLocation="CommercialFinanceCommonLeafTypesCust.xsd"/>
   <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.4.0" schemaLocation="CommonLeafTypesCust.xsd"/>
   <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.4.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
   <xs:annotation>
      <xs:documentation>Общие сложные типы  коммерческих и финансовых документов</xs:documentation>
   </xs:annotation>
   <xs:complexType name="AdditionalGoodsDescriptionType">
      <xs:annotation>
         <xs:documentation>Дополнительные характеристики товаров</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="Manufacturer" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Сведения о производителе</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="TradeMark" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Тов. знак, место происхождения, объект авторского права, смежных прав, патент и т.д.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GoodsMark" type="clt_ru:Text50Type">
            <xs:annotation>
               <xs:documentation>Марка товара</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GoodsModel" type="clt_ru:Text50Type">
            <xs:annotation>
               <xs:documentation>Модель товара</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GoodsStandart" type="clt_ru:Text50Type">
            <xs:annotation>
               <xs:documentation>Стандарт (ГОСТ, ОСТ, СПП, СТО, ТУ)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BankMarkType">
      <xs:annotation>
         <xs:documentation>Отметки банка </xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="BankName" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Наименование банка</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="BIC" type="clt_ru:BICIDType">
            <xs:annotation>
               <xs:documentation>БИК</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="RecipSign" type="cat_ru:PersonSignatureType">
            <xs:annotation>
               <xs:documentation>Подпись ответственного исполнителя  </xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BankType">
      <xs:annotation>
         <xs:documentation>Реквизиты банка</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="BankName" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Наименование банка</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="BIC" type="clt_ru:BICIDType">
            <xs:annotation>
               <xs:documentation>БИК</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CorrespAccount" type="clt_ru:AccountType">
            <xs:annotation>
               <xs:documentation>Номер корреспондентского счета (субсчета)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="BankPlace" type="clt_ru:PlaceNameType">
            <xs:annotation>
               <xs:documentation>Местонахождение банка</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="PersonalAccount" type="clt_ru:AccountType">
            <xs:annotation>
               <xs:documentation>Лицевой счет клиента</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ContractDeliveryTermsType">
      <xs:annotation>
         <xs:documentation>Описание условий поставки в контракте</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="DeliveryPlace" type="clt_ru:PlaceNameType">
            <xs:annotation>
               <xs:documentation>Наименование пункта поставки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="DeliveryTermsNumericCode" type="clt_ru:DeliveryTermsNumericCodeType">
            <xs:annotation>
               <xs:documentation>Числовой код условий поставки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DeliveryTermsStringCode" type="clt_ru:IncotermsDeliveryStringCodeType">
            <xs:annotation>
               <xs:documentation>Код условий поставки по Инкотермс</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="TermsDescription" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Описание, комментарии по условиям поставки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="InvoiceSaleTerms" type="catComFin_ru:InvoiceSaleTermsType">
            <xs:annotation>
               <xs:documentation>Срок оплаты по инвойсу</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ContractDescriptionType">
      <xs:annotation>
         <xs:documentation>Описание условий контракта</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="catComFin_ru:ImportContractTermsType">
            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="ExchangeClause" type="clt_ru:FreeText250Type">
                  <xs:annotation>
                     <xs:documentation>Сведения о предусмотренном условиями Контракта порядке пересчета валюты платежа в валюту цены </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="ContractSubject" type="clt_ru:FreeText250Type">
                  <xs:annotation>
                     <xs:documentation>Предмет контракта</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="ContractTime" type="clt_ru:DateCustType">
                  <xs:annotation>
                     <xs:documentation>Срок действия контракта </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="AdditionalContractTime" type="clt_ru:DateCustType">
                  <xs:annotation>
                     <xs:documentation>Дополнительный срок действия контракта (применяется в случае автоматического пролонгирования)</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="OtherTerms" type="clt_ru:FreeText250Type">
                  <xs:annotation>
                     <xs:documentation>Прочие условия сделки.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="ContractText" type="clt_ru:FreeText250Type">
                  <xs:annotation>
                     <xs:documentation>Текст контракта</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="DiscountPercentage" type="clt_ru:ProcentNumericType">
                  <xs:annotation>
                     <xs:documentation>Размер скидки в процентах</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="PaymentPeriod" type="clt_ru:Text50Type">
                  <xs:annotation>
                     <xs:documentation>Срок оплаты</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="DealSign" type="clt_ru:DigitalIndicatorType">
                  <xs:annotation>
                     <xs:documentation>Признак сделки купли/продажи (1 - купля, 0 - продажа)</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="PrepaySign" type="clt_ru:IndicatorType">
                  <xs:annotation>
                     <xs:documentation>Признак наличия предоплаты</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="StockCategorySign" type="clt_ru:IndicatorType">
                  <xs:annotation>
                     <xs:documentation>Признак отнесения товаров (предмета контракта) к категории биржевых</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="BuyerLimitationSign" type="clt_ru:IndicatorType">
                  <xs:annotation>
                     <xs:documentation>Признак наличия ограничений в отношении прав покупателя</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="InsuranceSign" type="clt_ru:IndicatorType">
                  <xs:annotation>
                     <xs:documentation>Признак наличия страховки товаров</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="ChangeContract" type="cat_ru:DocumentBaseType">
                  <xs:annotation>
                     <xs:documentation>Номера и даты изменений/дополнений к контракту.  </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ContractRegistrationType">
      <xs:annotation>
         <xs:documentation>Сведения о подписании (заключении) контракта (дата, номер, место)</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:DocumentBaseType">
            <xs:sequence>
               <xs:element minOccurs="0" name="ContractPlace" type="clt_ru:PlaceNameType">
                  <xs:annotation>
                     <xs:documentation>Место заключения контракта</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ContractSpecificationType">
      <xs:annotation>
         <xs:documentation>Спецификация к контракту</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ID" type="clt_ru:DocumentNumberType">
            <xs:annotation>
               <xs:documentation>Номер документа</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IssueDate" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата составления документа</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" name="SpecificationGoodsExtended" type="catComFin_ru:SpecificationGoodsExtendedType">
            <xs:annotation>
               <xs:documentation>Описание и дополнительные характеристики товаров из спецификации к контракту</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CustomerType">
      <xs:annotation>
         <xs:documentation>Реквизиты организации</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="OrganizationName" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Наименование клиента</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="INN" type="clt_ru:INNIDType">
            <xs:annotation>
               <xs:documentation>ИНН клиента</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="KPP" type="clt_ru:KPPCodeType">
            <xs:annotation>
               <xs:documentation>Код постановки на учет (КПП) клиента</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Bank" type="catComFin_ru:BankType">
            <xs:annotation>
               <xs:documentation>Реквизиты банка клиента</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DepartmentalInfoType">
      <xs:annotation>
         <xs:documentation>Ведомственная информация</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="DrawerStatus" type="clt_ru:CodeVariable2Type">
            <xs:annotation>
               <xs:documentation>Статус составителя расчетного документа</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CBC" type="cltComFin_ru:CBCType">
            <xs:annotation>
               <xs:documentation>Код бюджетной классификации (КБК), по которому учитывается налог (сбор)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="OKATO" type="clt_ru:OKATOCodeType">
            <xs:annotation>
               <xs:documentation>Код муниципального образования ОКАТО</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="PaymentReason" type="clt_ru:CodeVariable2Type">
            <xs:annotation>
               <xs:documentation>Основание налогового платежа</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="TaxPeriod" type="clt_ru:Text10Type">
            <xs:annotation>
               <xs:documentation>Налоговый период</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="DocNo" type="cltComFin_ru:TaxDocIDTextType">
            <xs:annotation>
               <xs:documentation>Номер налогового документа/Идентификатор сведений о физическом лице</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="DocDate" type="cltComFin_ru:TaxDocDateCodeTextType">
            <xs:annotation>
               <xs:documentation>Дата налогового документа</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="TaxPaymentKind" type="clt_ru:CodeVariable2Type">
            <xs:annotation>
               <xs:documentation>Тип налогового платежа</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CustomsCode" type="clt_ru:CustomsCodeType">
            <xs:annotation>
               <xs:documentation>Код таможенного органа</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ForeignPersonType">
      <xs:annotation>
         <xs:documentation>Реквизиты иностранной стороны контракта</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="Phone" type="clt_ru:PhoneNumberType">
            <xs:annotation>
               <xs:documentation>Телефон иностранной организации</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OrganizationName" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Наименование организации.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PostalAddress" type="cat_ru:AddressType">
            <xs:annotation>
               <xs:documentation>Почтовый адрес</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="GoodsPlaceType">
      <xs:annotation>
         <xs:documentation>Описание грузовых мест</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="PlaceKind" type="clt_ru:PackageCodeType">
            <xs:annotation>
               <xs:documentation>Код вида грузового места из справочника НСИ</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PlacesQuantity" type="clt_ru:PackagesQuantityType">
            <xs:annotation>
               <xs:documentation>Общее количество грузовых мест товара</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PlaceWeightQuantity" type="clt_ru:QuantityBasisType">
            <xs:annotation>
               <xs:documentation>Вес места (кг)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="GoodsWeightType">
      <xs:annotation>
         <xs:documentation>Вес нетто,брутто товара в кг.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="NetWeightQuantity" type="clt_ru:QuantityBasisType">
            <xs:annotation>
               <xs:documentation>Вес товара нетто (кг)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GrossWeightQuantity" type="clt_ru:QuantityBasisType">
            <xs:annotation>
               <xs:documentation>Вес товара брутто (кг)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ImportContractTermsType">
      <xs:annotation>
         <xs:documentation>Реквизиты и условия контракта по Импорту</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="Amount" type="clt_ru:Quantity18point3Type">
            <xs:annotation>
               <xs:documentation>Общая сумма сделки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Discount" type="clt_ru:Quantity18point3Type">
            <xs:annotation>
               <xs:documentation>Общая сумма скидки по контракту</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CurrencyCode" type="clt_ru:CurrencyA3CodeType">
            <xs:annotation>
               <xs:documentation>Код валюты контракта (цены)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="PaymentModeCode" type="cltComFin_ru:PaymentModeCodeType">
            <xs:annotation>
               <xs:documentation>Код расчетного срока оплаты товара (услуги) по Контракту </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="DueDateCode" type="clt_ru:Numeric1Type">
            <xs:annotation>
               <xs:documentation>Код применяемой формы расчетов по Контракту</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="LastDate" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Последняя дата расчетов по контракту</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="PaymentCurrencyCode" type="clt_ru:CurrencyA3CodeType">
            <xs:annotation>
               <xs:documentation>Код валюты платежа</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="InvoiceGoodsType">
      <xs:annotation>
         <xs:documentation>Сведения о товарах инвойса</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="GoodMarking" type="clt_ru:GoodMarkingType">
            <xs:annotation>
               <xs:documentation>Артикул (код товара, номер по каталогу производителя) </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GoodsCode" type="clt_ru:GoodsNomenclatureCodeType">
            <xs:annotation>
               <xs:documentation>Код товара по ТН ВЭД ТС</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="SupplementaryQualifierName" type="clt_ru:MeasureUnitQualifierNameType">
            <xs:annotation>
               <xs:documentation>Название дополнительной единицы измерения при возможности ее указания</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="4" name="GoodsDescription" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Описание товара текстовое (содержит название марок, моделей товаров и/или их индивидуальные номера - приказ ГТК РФ от 13.05.03 №500)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GoodsQuantity" type="clt_ru:QuantityBasisType">
            <xs:annotation>
               <xs:documentation>Количество товара, услуг без указания единицы измерения </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="MeasureUnitQualifierName" type="clt_ru:MeasureUnitQualifierNameType">
            <xs:annotation>
               <xs:documentation>Название единицы измерения при возможности ее указания</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GrossWeightQuantity" type="clt_ru:QuantityBasisType">
            <xs:annotation>
               <xs:documentation>Вес товара, брутто (кг)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="NetWeightQuantity" type="clt_ru:QuantityBasisType">
            <xs:annotation>
               <xs:documentation>Вес товара, нетто (кг)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Price" type="cltComFin_ru:PriceType">
            <xs:annotation>
               <xs:documentation>Цена товара/услуги</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="TotalCost" type="clt_ru:AmountType">
            <xs:annotation>
               <xs:documentation>Стоимость всего количества поставляемого (отгруженного) по счету - фактуре товара (выполненных работ, оказанных услуг) без налога</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Discount" type="clt_ru:AmountType">
            <xs:annotation>
               <xs:documentation>Cумма скидки</xs:documentation>
            </xs:annotation>
