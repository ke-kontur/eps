<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="urn:customs.ru:Information:CustomsDocuments:KTDOutCustomsMark:5.4.0" version="5.4.0.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.4.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.4.0" xmlns:kocm="urn:customs.ru:Information:CustomsDocuments:KTDOutCustomsMark:5.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.4.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
   <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.4.0" schemaLocation="CommonLeafTypesCust.xsd"/>
   <xs:annotation>
      <xs:documentation>Служебные отметки КДТ</xs:documentation>
   </xs:annotation>
   <xs:element name="KDTOutCustomsMark" type="kocm:KDTOutCustomsMarkType"/>
   <xs:complexType name="KDTOutCustomsMarkType">
      <xs:annotation>
         <xs:documentation>Служебные отметки КДТ</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:BaseDocType">
            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Comments" type="clt_ru:FreeText250Type">
                  <xs:annotation>
                     <xs:documentation>Комментарий</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="GTDDocumentID" type="clt_ru:DocumentIDType">
                  <xs:annotation>
                     <xs:documentation>Уникальный идентификатор КДТ к которой относятся служебные отметки (GUID)</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="KTDNumber" type="clt_ru:Count2Type">
                  <xs:annotation>
                     <xs:documentation>Порядковый номер КДТ</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="KDTOutCommonMark" type="kocm:KDTOutCommonMarkType">
                  <xs:annotation>
                     <xs:documentation>Общие отметки КДТ</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="KDTOutGoodsResolution" type="kocm:KDTOutGoodsResolutionType">
                  <xs:annotation>
                     <xs:documentation>Служебные отметки КДТ по товарам</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="GTDID" type="cat_ru:GTDIDType">
                  <xs:annotation>
                     <xs:documentation>Регистрационный номер ДТ.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
            <xs:attribute fixed="1006066E" name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required">
               <xs:annotation>
                  <xs:documentation>Идентификатор вида документа</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:annotation>
      <xs:documentation>Локальные прикладные типы.</xs:documentation>
   </xs:annotation>
   <xs:complexType name="KDTOutCommonMarkType">
      <xs:annotation>
         <xs:documentation>Общие служебные отметки КДТ</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="GRNumber" type="clt_ru:CodeVariable2Type">
            <xs:annotation>
               <xs:documentation>Обозначение графы (подраздела графы)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="StageMarkIdentifier" type="clt_ru:LetterIndicatorType">
            <xs:annotation>
               <xs:documentation>Идентификатор проставленной технологической отметки - 1 (номер этапа)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NoteMarkIdentifier" type="clt_ru:Code2Type">
            <xs:annotation>
               <xs:documentation>Идентификатор проставленной технологической отметки  -  2 (тип примечания)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MarkDescription" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Служебная запись</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="DateInf" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата проставления отметки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="TimeInf" type="clt_ru:TimeCustType">
            <xs:annotation>
               <xs:documentation>Время</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PersonName" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>ФИО инспектора, заверившего запись</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="LNP" type="clt_ru:LNPIDType">
            <xs:annotation>
               <xs:documentation>Номер ЛНП инспектора, заверившего запись</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="KDTOutGoodsResolutionType">
      <xs:annotation>
         <xs:documentation>Служебные отметки по товарам</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="GoodsNumeric" type="clt_ru:Count3Type">
            <xs:annotation>
               <xs:documentation>Номер товара по КДТ</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="GRNumber" type="clt_ru:CodeVariable2Type">
            <xs:annotation>
               <xs:documentation>Обозначение графы (подраздела графы)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="StageMarkIdentifier" type="clt_ru:LetterIndicatorType">
            <xs:annotation>
               <xs:documentation>Идентификатор проставленной технологической отметки - 1 (номер этапа)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NoteMarkIdentifier" type="clt_ru:Code2Type">
            <xs:annotation>
               <xs:documentation>Идентификатор проставленной технологической отметки  -  2 (тип примечания)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MarkDescription" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Служебная запись</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="DateInf" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата проставления отметки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="TimeInf" type="clt_ru:TimeCustType">
            <xs:annotation>
               <xs:documentation>Время</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PersonName" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>ФИО инспектора, заверившего запись</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="LNP" type="clt_ru:LNPIDType">
            <xs:annotation>
               <xs:documentation>Номер ЛНП инспектора, заверившего запись</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
</xs:schema>
