<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="urn:customs.ru:Information:RegistrationDocuments:NaturalPersonRegistrationCertificate:5.4.0" version="5.4.0.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.4.0" xmlns:cltReg_ru="urn:customs.ru:Information:RegistrationDocuments:RegistrationCommonLeafTypes:4.8.1" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.4.0" xmlns:nprc="urn:customs.ru:Information:RegistrationDocuments:NaturalPersonRegistrationCertificate:5.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:import namespace="urn:customs.ru:Information:RegistrationDocuments:RegistrationCommonLeafTypes:4.8.1" schemaLocation="RegistrationCommonLeafTypesCust.xsd"/>
   <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.4.0" schemaLocation="CommonLeafTypesCust.xsd"/>
   <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.4.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
   <xs:annotation>
      <xs:documentation>Свидетельство о государственной регистрации физического лица в качестве индивидуального предпринимателя по форме Р61001.</xs:documentation>
   </xs:annotation>
   <xs:element name="NaturalPersonRegistrationCertificate" type="nprc:NaturalPersonRegistrationCertificateType"/>
   <xs:complexType name="NaturalPersonRegistrationCertificateType">
      <xs:annotation>
         <xs:documentation>Свидетельство о государственной регистрации физического лица в качестве индивидуального предпринимателя по форме Р61001</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:BaseDocType">
            <xs:sequence>
               <xs:element name="RegistrationDate" type="clt_ru:DateCustType">
                  <xs:annotation>
                     <xs:documentation>Дата государственной регистрации физического лица в качестве индивидуального предпринимателя и данные документа</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Businessman" type="clt_ru:NameType">
                  <xs:annotation>
                     <xs:documentation>Фамилия, имя, отчество предпринимателя</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="RegistrationOrganization" type="clt_ru:NameType">
                  <xs:annotation>
                     <xs:documentation>Наименование регистрирующего органа</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="RegistrationNumberID" type="cltReg_ru:NaturalPersonRegistrationIDType">
                  <xs:annotation>
                     <xs:documentation>Государственный регистрационный номер записи, вносимой в Единый государственный реестр индивидуальных предпринимателей</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="FormName" type="clt_ru:Code6Type">
                  <xs:annotation>
                     <xs:documentation>Форма</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="DocumentSignature" type="cat_ru:PersonBaseType">
                  <xs:annotation>
                     <xs:documentation>Подпись уполномоченного лица регистрирующего органа</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Form" type="nprc:FormType">
                  <xs:annotation>
                     <xs:documentation>Серия и номер бланка</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
            <xs:attribute fixed="1001201E" name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required">
               <xs:annotation>
                  <xs:documentation>Идентификатор вида документа</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:annotation>
      <xs:documentation>Локальные прикладные типы.</xs:documentation>
   </xs:annotation>
   <xs:complexType name="FormType">
      <xs:annotation>
         <xs:documentation>Серия и номер бланка</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="FormSerial" type="clt_ru:Code2Type">
            <xs:annotation>
               <xs:documentation>Серия бланка</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FormNumber" type="clt_ru:Code9Type">
            <xs:annotation>
               <xs:documentation>Номер бланка</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
</xs:schema>
