<?xml version="1.0" encoding="UTF-8" ?>
<beans 
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
        					http://www.springframework.org/schema/beans/spring-beans-2.5.xsd" >
	
	<bean id="adm12017" class="ru.acs.fts.eps2.engine.processing.JobBatch" >
		<property name="description" value="ADM.12017 (локальное РТУ)" />
		<property name="preparedJobs">
			<list>
			
				<!-- Проверяем наличие в базе конверта с таким же EnvelopeID, DocumentID -->                
                <bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
                	<constructor-arg ref="checkSameEnvelopeExists" />
                	<constructor-arg>
                		<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
                			<constructor-arg>
                				<map />
                			</constructor-arg>
                		</bean>
                	</constructor-arg>
                </bean>            			

				<!-- Сохраняем входящий конверт -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveIncomeEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>

				<!-- CMN.00002 -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="callSubBatch" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="subBatch" value-ref="sendCMN00002" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>				

				<!-- Авторизация (от КАСТО декларанту в контексте процедуры) -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="authorization" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="authType" value="FROM_EPS_TO_DECL_WITHOUT_CODE" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Структурный контроль -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="structControlRequest" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Проверка подписи -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="checkSignedEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Проверка содержимого конверта -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="checkMessageContent" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Переводим состояне процедуры (если флаг udFlagNotOK не выставлен) -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="changeProcessState" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="eventName" value="DIFF_RTU_EVENT" />
									<entry key="ignoreChangeState" value="udFlagNotOK" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Создаем транзитное сообщение ADM.15005 сохраняя заголовок и тело исходного сообщения -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="createTransitMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="msg12017" />
									<entry key="messageType" value="MSG.12017" />
									<entry key="receivingSystem" value="CUSTOMS" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="sendMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="msg15002" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveObjectToDb" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="objectToSave" value="msg12017" />
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				
				<!-- Отправляем ADM.00006 -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="createNotificationMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="adm00006" />
									<entry key="messageType" value="ADM.00006" />
									<entry key="receivingSystem" value="EPS" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="signEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="adm00006" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="sendMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="adm00006" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveObjectToDb" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="objectToSave" value="adm00006" />
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Сохраняем переведенное состояние процедуры в базу -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="updateProcessInfo" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
			</list>
		</property>
	</bean>
</beans>