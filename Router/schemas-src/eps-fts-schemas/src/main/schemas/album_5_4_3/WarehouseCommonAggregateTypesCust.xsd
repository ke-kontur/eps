<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="urn:customs.ru:Information:WarehouseDocuments:WarehouseCommonAggregateTypesCust:5.4.0" version="5.4.0.0" xmlns:catWH_ru="urn:customs.ru:Information:WarehouseDocuments:WarehouseCommonAggregateTypesCust:5.4.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.4.0" xmlns:cltWH_ru="urn:customs.ru:Information:WarehouseDocuments:WarehouseCommonLeafTypesCust:5.0.5" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.4.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
   <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.4.0" schemaLocation="CommonLeafTypesCust.xsd"/>
   <xs:import namespace="urn:customs.ru:Information:WarehouseDocuments:WarehouseCommonLeafTypesCust:5.0.5" schemaLocation="WarehouseCommonLeafTypesCust.xsd"/>
   <xs:annotation>
      <xs:documentation>Общие сложные типы документов СВХ</xs:documentation>
   </xs:annotation>
   <xs:complexType name="AISTSDocumentIDType">
      <xs:annotation>
         <xs:documentation>Справочный номер документа</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="CustomsCode" type="clt_ru:Code5Type">
            <xs:annotation>
               <xs:documentation>Код таможенного органа регистрации документа</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IssueDate" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата оформления документа</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DocNumber" type="clt_ru:Code5Type">
            <xs:annotation>
               <xs:documentation>Номер документа</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ArriveCustomsDocType">
      <xs:annotation>
         <xs:documentation>Таможенные документы</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:DocumentBaseType">
            <xs:sequence>
               <xs:element minOccurs="0" name="DocumentCode" type="clt_ru:Code2Type">
                  <xs:annotation>
                     <xs:documentation>Код таможенного документа</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="AviaParticularsType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания подробностей для авиационного транспорта</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="FlightNumber" type="cltWH_ru:FlightNumber">
            <xs:annotation>
               <xs:documentation>Номер рейса</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="FlightDate" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата рейса</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AvtoParticularsType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания подробностей для автомобильного транспорта</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="TrailerIdentifier" type="clt_ru:TransportMeansIDType">
            <xs:annotation>
               <xs:documentation>Идентификатор (номер) прицепного транспортного средства</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="SecondTrailerIdentifier" type="clt_ru:TransportMeansIDType">
            <xs:annotation>
               <xs:documentation>Идентификатор (номер) второго прицепного транспортного средства</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CWDocType">
      <xs:annotation>
         <xs:documentation>Базовый тип для СВХ</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:BaseDocType">
            <xs:sequence>
               <xs:element name="DocumentDateTime" type="clt_ru:DateTimeCustType">
                  <xs:annotation>
                     <xs:documentation>Дата и время создания электронного документа.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="CustomsProperties" type="cat_ru:CustomsType">
                  <xs:annotation>
                     <xs:documentation>Реквизиты таможенного органа</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ConsigneeType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания грузополучателя</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:OrganizationBaseType">
            <xs:sequence>
               <xs:element name="Address" type="catWH_ru:WHAddressType">
                  <xs:annotation>
                     <xs:documentation>Адрес получателя</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="IdentityCard" type="cat_ru:IdentityCardType">
                  <xs:annotation>
                     <xs:documentation>Документ, удостоверяющий личность получателя. Заполняется в случае, если получатель - физ. лицо</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ConsignorType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания грузоотправителя</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="CountryCode" type="clt_ru:CountryN3CodeType">
            <xs:annotation>
               <xs:documentation>Цифровой код страны отправителя  (по справочнику ОКСМ)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="OrganizationName" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Наименование организации / ФИО физического лица</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Address" type="catWH_ru:WHAddressType">
            <xs:annotation>
               <xs:documentation>Адрес</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="GoodsLoadingPlaceType">
      <xs:annotation>
         <xs:documentation>Местонахождение товаров</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="CustomsCode" type="clt_ru:CustomsCodeType">
            <xs:annotation>
               <xs:documentation>Код таможенного органа, в зоне деятельности которого находятся товары</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice>
            <xs:element name="Warehouse" type="catWH_ru:WarehouseType">
               <xs:annotation>
                  <xs:documentation>Сведения о  СВХ на котором находятся товары</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="OtherLoadingPlace" type="catWH_ru:OtherLoadingPlaceType">
               <xs:annotation>
                  <xs:documentation>Иное место нахождения товаров</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="MPONumberType">
      <xs:annotation>
         <xs:documentation>Номер МПО</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ServiceIndicator" type="clt_ru:CodeVariable2Type">
            <xs:annotation>
               <xs:documentation>Указатель сервиса</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SerialNumber" type="clt_ru:Code9Type">
            <xs:annotation>
               <xs:documentation>Порядковый номер</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CountryCode" type="clt_ru:CountryA2CodeType">
            <xs:annotation>
               <xs:documentation>Код страны</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="OtherLoadingPlaceType">
      <xs:annotation>
         <xs:documentation>Иное место нахождения товаров</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="GoodsLocationPlaceDesc" type="clt_ru:PlaceNameType">
            <xs:annotation>
               <xs:documentation>Место нахождения товаров/СКЛАД ПОЛУЧАТЕЛЯ (кроме названия ж/д cтанции)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="RailwayStationCode" type="clt_ru:RailwayStationCodeType">
            <xs:annotation>
               <xs:documentation>Код ж/д станции местонахождения товара.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="RailwayStationName" type="clt_ru:PlaceNameType">
            <xs:annotation>
               <xs:documentation>Наименование ж/д станции местонахождения товара.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Address" type="cat_ru:AddressType">
            <xs:annotation>
               <xs:documentation>Адрес</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="RailRoadParticularsType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания подробностей для железнодорожного транспорта</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="RailStation" type="clt_ru:RailwayStationCodeType">
            <xs:annotation>
               <xs:documentation>Код станции отправления (для прибытия) или назначения (для убытия)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="SeaParticularsType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания подробностей для морского/речного транспорта</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="EntryNumber" type="cltWH_ru:SeaEntryNumberType">
            <xs:annotation>
               <xs:documentation>Номер прихода/ухода</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CountryCode" type="clt_ru:CountryN3CodeType">
            <xs:annotation>
               <xs:documentation>Данные о стране принадлежности судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Captain" type="cat_ru:PersonBaseType">
            <xs:annotation>
               <xs:documentation>Данные о капитане</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="TransportDocRegNumType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания регистрационного номера транспортного документа</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:GTDIDType">
            <xs:sequence>
               <xs:element name="WaybillNumPP" type="clt_ru:CodeVariable3Type">
                  <xs:annotation>
                     <xs:documentation>Порядковый номер транспортного документа по журналу регистрации</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="WHAddressType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для адреса организации</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:AddressType">
            <xs:sequence>
               <xs:element minOccurs="0" name="AddressLine" type="clt_ru:FreeText250Type">
                  <xs:annotation>
                     <xs:documentation>Информация об адресе организации в виде одной строки</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="WHAngarType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для расширенного описания данных о размещении товара на СВХ</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="catWH_ru:WHPlaceBaseType">
            <xs:sequence>
               <xs:element minOccurs="0" name="Parking" type="cltWH_ru:SVHPlaceType">
                  <xs:annotation>
                     <xs:documentation>Парковочное место</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="Angar" type="cltWH_ru:SVHPlaceType">
                  <xs:annotation>
                     <xs:documentation>Ангар</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="WHPackind" type="cltWH_ru:SVHPlaceType">
                  <xs:annotation>
                     <xs:documentation>Номер стеллажа или полки (для товара) / номер места в ангаре (для контейнера)</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="Cell" type="cltWH_ru:SVHPlaceType">
                  <xs:annotation>
                     <xs:documentation>Номер ячейки (заполняется при указании сведений о товаре)</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="WHCargoPlaceType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания грузовых мест товара</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="PlaceNumber" type="clt_ru:Quantity8Type">
            <xs:annotation>
               <xs:documentation>Количество грузовых мест</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="PackageCode" type="clt_ru:PackageCodeType">
            <xs:annotation>
               <xs:documentation>Код вида упаковки (груза)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="PlaceDescription" type="clt_ru:Text40Type">
            <xs:annotation>
               <xs:documentation>Вид  грузовых  мест - описание</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="WHCarrierType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания организации-перевозчика</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:OrganizationBaseType">
            <xs:sequence>
               <xs:element name="CountryCode" type="clt_ru:CountryN3CodeType">
                  <xs:annotation>
                     <xs:documentation>Цифровой код страны перевозчика (по справочнику ОКСМ)</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Address" type="catWH_ru:WHAddressType">
                  <xs:annotation>
                     <xs:documentation>Адрес лица, помещающего  товары на ВХ</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="CarrierPerson" type="cat_ru:PersonBaseType">
                  <xs:annotation>
                     <xs:documentation>Представитель перевозчика</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="WHContainerExtendedType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для расширенного описания контейнера</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="catWH_ru:WHContainerInformationType">
            <xs:sequence>
               <xs:element minOccurs="0" name="KeepingPlace" type="catWH_ru:WHAngarType">
                  <xs:annotation>
                     <xs:documentation>Информация о хранении контейнера</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="WHContainerInformationType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания контейнера</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ContainerNumber" type="clt_ru:ContainerIdentityType">
            <xs:annotation>
               <xs:documentation>Номер контейнера.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GrossWeightQuantity" type="clt_ru:QuantityBasisType">
            <xs:annotation>
               <xs:documentation>Вес брутто (кг) всех товаров в контейнере</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="WHCustomInspectorType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания должностного лица таможенного органа</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:PersonBaseType">
            <xs:sequence>
               <xs:element name="LNP" type="clt_ru:LNPIDType">
                  <xs:annotation>
                     <xs:documentation>Л.Н.П. сотрудника таможенного органа, зарегистрировавшего отчет</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="WHCustomRegisterDocumentDataType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для данных о регистрации документа в таможенном органе</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="CustomState" type="cltWH_ru:CustomDocumentState">
            <xs:annotation>
               <xs:documentation>Таможенный статус отчета</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="RegisterTime" type="clt_ru:TimeCustType">
            <xs:annotation>
               <xs:documentation>Время регистрации отчета в журнале регистрации ДО таможенного органа</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Inspector" type="catWH_ru:WHCustomInspectorType">
            <xs:annotation>
               <xs:documentation>Сотрудник таможенного органа, зарегистрировавший отчет</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Cancelation" type="catWH_ru:WHDocumentCancelationType">
            <xs:annotation>
               <xs:documentation>Данные об аннулировании отчета</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="RegisterNumber" type="catWH_ru:WHDocumentRegisterNumberType">
            <xs:annotation>
               <xs:documentation>Регистрационный номер отчета по журналу регистрации ДО таможенного органа</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="WHCustomsControlType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для описания cведений о мере таможенного контроля над хранимым на СВХ товаром</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Kind" type="cltWH_ru:CustomsControlKind">
            <xs:annotation>
               <xs:documentation>Вид меры ТК</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Date" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата применения</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Reason" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Основание применения меры / Информация по грузу</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="KeepingLimit" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Срок ВХ по принятой мере ТК</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Inspector" type="catWH_ru:WHCustomInspectorType">
            <xs:annotation>
               <xs:documentation>Сотрудник таможенного органа, применивший меру ТК</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="WHDocumentCancelationType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для данных об аннулировании документа</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="CancelDate" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата аннулирования</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CancelReason" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Причина аннулирования</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Inspector" type="catWH_ru:WHCustomInspectorType">
            <xs:annotation>
               <xs:documentation>Сотрудник таможенного органа, аннулировавший отчет</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="WHDocumentRegisterNumberType">
      <xs:annotation>
         <xs:documentation>Комплексный тип для регистрационного номера отчета ДО1/ДО2/ДО3 по журналу регистрации ДО таможенного органа</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:GTDIDType">
            <xs:sequence>
               <xs:choice>
                  <xs:element name="CertificateNumber" type="clt_ru:DocumentNumberType">
                     <xs:annotation>
                        <xs:documentation>Номер свидетельства о включении в реестр владельцев временного хранения</xs:documentation>
                     </xs:annotation>
                  </xs:element>
