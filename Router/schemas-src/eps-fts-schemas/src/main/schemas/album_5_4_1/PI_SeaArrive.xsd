         <xs:element maxOccurs="unbounded" minOccurs="0" name="ArmsInfo" type="pisa:ProhibitedSubjectInfoType">
            <xs:annotation>
               <xs:documentation>Информация о наличии оружия и боеприпасов</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="DrugsSubstanceInfo" type="pisa:DrugsSubstanceInfoType">
            <xs:annotation>
               <xs:documentation>Сведения о наличии лекарственных средств</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="CurrencyInfo" type="pisa:CurrencyInfoType">
            <xs:annotation>
               <xs:documentation>Информация о валюте и валютных ценностях</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="PermitionDocs" type="cat_ru:DocumentBaseType">
            <xs:annotation>
               <xs:documentation>Информация о разрешительных документах</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ProhibitedSubjectInfoType">
      <xs:annotation>
         <xs:documentation>Информация о товаре, на который накладывается запрет или ограничение</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:SupplementaryQuantityType">
            <xs:sequence>
               <xs:element name="Name" type="clt_ru:NameType">
                  <xs:annotation>
                     <xs:documentation>Наименование</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="Description" type="clt_ru:FreeText250Type">
                  <xs:annotation>
                     <xs:documentation>Описание товара</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
