<?xml version="1.0" encoding="UTF-8" ?>
<beans 
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
        					http://www.springframework.org/schema/beans/spring-beans-2.5.xsd" >
        					
    <bean id="adm12016" class="ru.acs.fts.eps2.engine.processing.JobBatch" >
      	<property name="description" value="ADM.12016 (удаленное РТУ)" />
		<property name="preparedJobs">
            <list>
            
				<!-- Проверяем наличие в базе конверта с таким же EnvelopeID, DocumentID -->                
                <bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
                	<constructor-arg ref="checkSameEnvelopeExists" />
                	<constructor-arg>
                		<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
                			<constructor-arg>
                				<map />
                			</constructor-arg>
                		</bean>
                	</constructor-arg>
                </bean>            

				<!-- Сохраняем входящий конверт -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveIncomeEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>

				<!-- CMN.00002 -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="callSubBatch" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="subBatch" value-ref="sendCMN00002" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>				

            	<!-- Авторизация (от КАСТО декларанту в контексте процедуры) -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="authorization" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="authType" value="FROM_EPS_TO_KASTO_NOT_IN_PROCESS" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
								
				<!-- Структурный контроль -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="structControlRequest" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Проверка подписи -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="checkSignedEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>

				<!-- Проверка содержимого конверта -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="checkMessageContent" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
								
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="businessProcessing" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
                            <constructor-arg>
                                <map>
                                    <entry key="envelopeName" value="envelopes" />
                                </map>
                            </constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Переводим состояне процедуры (если флаг udFlagNotOK не выставлен) -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="changeProcessState" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="eventName" value="DIFF_RTU_EVENT" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="sendMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
                            <constructor-arg>
                                <map>
                                    <entry key="envelopeName" value="envelopes" />
                                </map>
                            </constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveObjectToDb" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="objectToSave" value="envelopes" />
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Отправляем ADM.00006 -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="createNotificationMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="adm00006" />
									<entry key="messageType" value="ADM.00006" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="signEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="adm00006" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="sendMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="adm00006" />
									<entry key="recvSystem" value="EPS" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveObjectToDb" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="objectToSave" value="adm00006" />
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Сохраняем переведенное состояние процедуры в базу -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="updateProcessInfo" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
            </list>
		</property>
   </bean>
</beans>