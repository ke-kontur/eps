               <xs:documentation>Название порта</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CrewInfoType">
      <xs:annotation>
         <xs:documentation>Сведения об экипаже</xs:documentation>
