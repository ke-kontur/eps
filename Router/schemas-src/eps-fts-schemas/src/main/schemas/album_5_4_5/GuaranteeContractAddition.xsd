<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="urn:customs.ru:Information:CommercialFinanceDocuments:GuaranteeContractAddition:5.4.0" version="5.4.0.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.4.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.4.0" xmlns:gcadd="urn:customs.ru:Information:CommercialFinanceDocuments:GuaranteeContractAddition:5.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.4.0" schemaLocation="CommonLeafTypesCust.xsd"/>
   <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.4.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
   <xs:annotation>
      <xs:documentation>Дополнение к договору поручительства</xs:documentation>
   </xs:annotation>
   <xs:element name="GuaranteeContractAddition" type="gcadd:GuaranteeContractAdditionType"/>
   <xs:complexType name="GuaranteeContractAdditionType">
      <xs:annotation>
         <xs:documentation>Дополнение к договору поручительства</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:BaseDocType">
            <xs:sequence>
               <xs:element name="DutyAmount" type="clt_ru:AmountType">
                  <xs:annotation>
                     <xs:documentation>Сумма обеспечения таможенных пошлин и налогов</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="CurrencyCode" type="clt_ru:CurrencyA3CodeType">
                  <xs:annotation>
                     <xs:documentation>Код валюты обеспечения</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ValidityDate" type="clt_ru:DateCustType">
                  <xs:annotation>
                     <xs:documentation>Cрок, до истечения которого возможно использование дополнения в целях обеспечения уплаты таможенных пошлин, налогов при таможенном транзите товаров.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Guarantor" type="gcadd:OrganizationInfoType">
                  <xs:annotation>
                     <xs:documentation>Сведения о поручителе</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="PaymentProvider" type="gcadd:OrganizationInfoType">
                  <xs:annotation>
                     <xs:documentation>Сведения о лице, предоставившем обеспечение уплаты таможенных пошлин, налогов </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Declarant" type="gcadd:OrganizationInfoType">
                  <xs:annotation>
                     <xs:documentation>Сведения о декларанте</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="TransportDocument" type="gcadd:DocumentInfoType">
                  <xs:annotation>
                     <xs:documentation>Сведения о транспортном (перевозочном) документе</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="GuaranteeDocument" type="cat_ru:DocumentBaseType">
                  <xs:annotation>
                     <xs:documentation>Cведения о договоре поручительства</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="CommercialDocument" type="gcadd:DocumentInfoType">
                  <xs:annotation>
                     <xs:documentation>Сведения о коммерческом документе</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="ArrivalCustoms" type="cat_ru:CUCustomsType">
                  <xs:annotation>
                     <xs:documentation>Таможенный орган назначения</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="DepatureCustoms" type="cat_ru:CUCustomsType">
                  <xs:annotation>
                     <xs:documentation>Таможенный орган отправления</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="RegNumberDT" type="cat_ru:GTDIDType">
                  <xs:annotation>
                     <xs:documentation>Регистрационный номер таможенной декларации</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
            <xs:attribute fixed="1002024E" name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required">
               <xs:annotation>
                  <xs:documentation>Идентификатор вида документа</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:annotation>
      <xs:documentation>Локальные прикладные типы.</xs:documentation>
   </xs:annotation>
   <xs:complexType name="DocumentInfoType">
      <xs:annotation>
         <xs:documentation>Сведения о документе</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:DocumentBaseType">
            <xs:sequence>
               <xs:element name="DocumentCode" type="clt_ru:CustomsDocumentCodeType">
                  <xs:annotation>
                     <xs:documentation>Код видa  документа в соответствии с Классификатором видов документов, используемых при таможенном декларировании</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="OrganizationInfoType">
      <xs:annotation>
         <xs:documentation>Сведения об организации</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:OrganizationBaseType">
            <xs:sequence>
               <xs:element minOccurs="0" name="Address" type="cat_ru:AddressType">
                  <xs:annotation>
                     <xs:documentation>Адрес организации / физического лица</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="IdentityCard" type="cat_ru:IdentityCardType">
                  <xs:annotation>
                     <xs:documentation>Документ, удостоверяющий личность</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
</xs:schema>
