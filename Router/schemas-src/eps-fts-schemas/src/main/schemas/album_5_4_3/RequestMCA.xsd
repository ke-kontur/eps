	<xs:import namespace="urn:customs.ru:CUESADCommonAggregateTypesCust:5.4.0" schemaLocation="CUESADCommonAggregateTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CUESADCommonLeafTypes:5.4.0" schemaLocation="CUESADCommonLeafTypesCust.xsd"/>
	<xs:annotation>
		<xs:documentation>Информация о ДТ и документе по запросу</xs:documentation>
	</xs:annotation>
	<xs:element name="RequestMCA" type="itsri:RequestMCAType"/>
	<xs:complexType name="RequestMCAType">
		<xs:annotation>
			<xs:documentation>Информация о ДТ и документе по запросу</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:BaseDocType">
				<xs:sequence>
					<xs:element name="TransitDirectionCode" type="clt_ru:TransitDirectionCodeType">
