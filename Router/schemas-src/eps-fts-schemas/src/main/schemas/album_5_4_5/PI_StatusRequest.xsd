<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.4.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.4.0" xmlns:rpiid="urn:customs.ru:Information:PriorInformation:PI_StatusRequest:5.4.5" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:customs.ru:Information:PriorInformation:PI_StatusRequest:5.4.5" elementFormDefault="qualified" version="5.4.5.0">
	<xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.4.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CommonLeafTypes:5.4.0" schemaLocation="CommonLeafTypesCust.xsd"/>
	<xs:annotation>
		<xs:documentation>Запрос статуса предварительной информации</xs:documentation>
	</xs:annotation>
	<xs:element name="PI_StatusRequest" type="rpiid:PI_StatusRequestType"/>
	<xs:complexType name="PI_StatusRequestType">
		<xs:annotation>
			<xs:documentation>Запрос статуса предварительной информации</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:BaseDocType">
				<xs:sequence>
					<xs:element name="InformationID" type="clt_ru:DocumentIDType" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Идентификатор предварительной информации по которой запрашивается статус</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" default="1006809E">
					<xs:annotation>
						<xs:documentation>Идентификатор вида документа</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
