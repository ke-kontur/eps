      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="ShipIMONumber" type="cltpi_ru:IMONumberType">
            <xs:annotation>
               <xs:documentation>ИМО номер судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ShipName" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Наименование судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CapitainSurname" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Фамилия капитана судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="ShipCountryCode" type="clt_ru:CountryA2CodeType">
            <xs:annotation>
               <xs:documentation>Код страны принадлежности судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="ShipRegDate" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата регистрации судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="ShipRegNum" type="clt_ru:CodeVariable10Type">
            <xs:annotation>
               <xs:documentation>Номер регистрации судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="ShipRegPort" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Порт регистрации судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Flag" type="clt_ru:CountryNameType">
            <xs:annotation>
               <xs:documentation>Флаг судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Berth" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Место стоянки судна в порту</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GrossTonnage" type="clt_ru:WeighQuantityType">
            <xs:annotation>
               <xs:documentation>Валовая вместимость(тоннаж)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="NetTonnage" type="clt_ru:WeighQuantityType">
            <xs:annotation>
               <xs:documentation>Чистая вместимость(тоннаж)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="ShipOwnerName" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Наименование судовладельца</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ShipType" type="clt_ru:Text50Type">
            <xs:annotation>
               <xs:documentation>Тип судна</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ArrivalPlace" type="pisa:CountryPortInfoType">
