<?xml version="1.0" encoding="UTF-8" ?>
<beans 
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
							http://www.springframework.org/schema/beans/spring-beans-2.5.xsd" >

	<bean id="cmn11062RemoteIn" class="ru.acs.fts.eps2.engine.processing.JobBatch">
		<property name="description" value="Обработка сообщения CMN.11075 на внешнем РТУ" />
		<property name="preparedJobs">
			<list>

				<!-- Проверяем наличие в базе конверта с таким же EnvelopeID, DocumentID -->                
                <bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
                	<constructor-arg ref="checkSameEnvelopeExists" />
                	<constructor-arg>
                		<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
                			<constructor-arg>
                				<map />
                			</constructor-arg>
                		</bean>
                	</constructor-arg>
                </bean>
				
				<!-- Сохраняем входящий конверт -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveIncomeEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
			
				<!-- Авторизация -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="authorization" />
					<constructor-arg >
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="authType" value="FROM_EPS_TO_KASTO" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
			
				<!-- Проверка возможности перехода в текущем статусном состоянии процедуры -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="checkProcessTransmission" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="eventName" value="DIFF_RTU_EVENT" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>

				<!-- Структурный контроль -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="structControlRequest" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>

				<!-- Контроль ЭЦП -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="checkSignedEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Контроль соответствия типа сообщения и передаваемого документа -->
				<!-- Контроль типа передачи сообщения -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="checkMessageContent" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>				
				
				<!-- Создаем транзитное сообщение в таможню -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="createTransitMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="transitToCustoms" />
									<entry key="receivingSystem" value="CUSTOMS" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>		
				
				<!-- Создаем и отправляем ADM.00006 -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="createNotificationMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="adm00006Notif" />
									<entry key="messageType" value="ADM.00006" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>		

				<!-- Бизнес обработка -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="businessProcessing" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map/>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Отправляем транзитное сообщение в таможню -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="sendMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="transitToCustoms" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Сохраняем транзитное сообщение в базу -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveObjectToDb" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="objectToSave" value="transitToCustoms" />
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Переводим состояние процедуры -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob" >
					<constructor-arg ref="changeProcessState" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext" >
							<constructor-arg>
								<map>
									<entry key="eventName" value="DIFF_RTU_EVENT" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<!-- Сохраняем переведенное состояние процедуры в базу -->
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="updateProcessInfo" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="signEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="adm00006Notif" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="sendMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="adm00006Notif" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveObjectToDb" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="objectToSave" value="adm00006Notif" />
									<entry key="needFlush" value="true" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
			</list>
		</property>
	</bean>
</beans>