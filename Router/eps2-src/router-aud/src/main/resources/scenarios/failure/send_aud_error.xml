<?xml version="1.0" encoding="UTF-8" ?>
<beans 
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
							http://www.springframework.org/schema/beans/spring-beans-2.5.xsd" >
							
	<bean id="fs_send_aud_error" class="ru.acs.fts.eps2.engine.processing.JobBatch" >
		<property name="description" value="Отправка сообщения AUD.ERROR" />
		<property name="preparedJobs">
			<list>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="createAudError" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="audErrorEnvelope" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="signAudEnvelope" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="envelopeName" value="audErrorEnvelope" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="sendAudMessage" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map>
									<entry key="queueKey" value="OUTPUT_QUEUE" />
									<entry key="envelopeName" value="audErrorEnvelope" />
								</map>
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
				<bean class="ru.acs.fts.eps2.engine.processing.PreparedJob">
					<constructor-arg ref="saveProtocols" />
					<constructor-arg>
						<bean class="ru.acs.fts.eps2.engine.processing.JobContext">
							<constructor-arg>
								<map />
							</constructor-arg>
						</bean>
					</constructor-arg>
				</bean>
				
			</list>
		</property>
	</bean>
</beans>