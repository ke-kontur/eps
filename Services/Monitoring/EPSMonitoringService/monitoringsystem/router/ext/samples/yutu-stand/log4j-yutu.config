log4j.rootLogger=INFO, F2 , FDEB
log4j.appender.F2.Threshold = INFO

log4j.appender.F2=biz.minaret.log4j.DatedFileAppender
log4j.appender.F2.Directory=c:/Projects/JavaRouter/logs/
log4j.appender.F2.Prefix=yutu-router-
log4j.appender.F2.Suffix=.log

log4j.appender.F2.Append = true
log4j.appender.F2.ImmediateFlush=true
log4j.appender.F2.layout = org.apache.log4j.PatternLayout
log4j.appender.F2.layout.ConversionPattern = %d [%t] %-5p %c - %m%n

log4j.appender.FDEB.Threshold = DEBUG
log4j.appender.FDEB=biz.minaret.log4j.DatedFileAppender
log4j.appender.FDEB.Directory=c:/Projects/JavaRouter/logs/
log4j.appender.FDEB.Prefix=yutu-router-deb-
log4j.appender.FDEB.Suffix=.log

log4j.appender.FDEB.Append = true
log4j.appender.FDEB.ImmediateFlush=true
log4j.appender.FDEB.layout = org.apache.log4j.PatternLayout
log4j.appender.FDEB.layout.ConversionPattern = %d [%t] %-5p %c - %m%n

log4j.logger.ru=DEBUG
log4j.logger.org.springframework.jms.connection.JmsTransactionManager =INFO