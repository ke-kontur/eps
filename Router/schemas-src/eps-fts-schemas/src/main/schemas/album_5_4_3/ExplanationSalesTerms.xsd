<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="urn:customs.ru:Information:CustomsDocuments:ExplanationSalesTerms:5.4.0" version="5.4.0.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.4.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.4.0" xmlns:est="urn:customs.ru:Information:CustomsDocuments:ExplanationSalesTerms:5.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.4.0" schemaLocation="CommonLeafTypesCust.xsd"/>
   <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.4.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
   <xs:annotation>
      <xs:documentation>Пояснения по условиям продажи, которые могли повлиять на цену сделки</xs:documentation>
   </xs:annotation>
   <xs:element name="ExplanationSalesTerms" type="est:ExplanationSalesTermsType"/>
   <xs:complexType name="ExplanationSalesTermsType">
      <xs:annotation>
         <xs:documentation>Пояснения по условиям продажи, которые могли повлиять на цену сделки.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="cat_ru:BaseDocType">
            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Comments" type="clt_ru:FreeText250Type">
                  <xs:annotation>
                     <xs:documentation>Дополнительные данные</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="HistoryCooperation" type="est:HistoryCooperationType">
                  <xs:annotation>
                     <xs:documentation>История сотрудничества Продавца и Покупателя</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ChoiceOrderGoods" type="est:ChoiceOrderGoodsType">
                  <xs:annotation>
                     <xs:documentation>Выбор и заказ товара</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="PricingTransactions" type="est:PricingTransactionsType">
                  <xs:annotation>
                     <xs:documentation>Формирование цены сделки</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="InfoContractDeliveryTerms" type="est:InfoContractDeliveryTermsType">
                  <xs:annotation>
                     <xs:documentation>Сведения о договоре и условиях поставки</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="SaleGoodsDomesticMarket" type="est:SaleGoodsDomesticMarketType">
                  <xs:annotation>
                     <xs:documentation>Реализация товаров на внутреннем рынке</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="PersonSignature" type="cat_ru:PersonSignatureType">
                  <xs:annotation>
                     <xs:documentation>Подпись</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
            <xs:attribute fixed="1006038E" name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required">
               <xs:annotation>
                  <xs:documentation>Идентификатор вида документа</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:annotation>
      <xs:documentation>Локальные прикладные типы</xs:documentation>
   </xs:annotation>
   <xs:complexType name="AccountType">
      <xs:annotation>
         <xs:documentation>Какой по счету является партия товара,ввозимого по рассматриваемому контракту</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Type" type="clt_ru:DigitalIndicatorType">
            <xs:annotation>
               <xs:documentation>Тип партии товаров. "0" - единственная, "1" - первая, "2" - последняя, "3" - иное</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Other" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Номер партии при выборе значения "Иное"</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AlignmentPricesType">
      <xs:annotation>
         <xs:documentation>Согласование цены сделки.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Negotiations" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак согласования цены сделки путем личных переговоров представителей продавца и покупателя</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Correspondence" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак согласования цены сделки по переписке</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NegotiationsCorrespondence" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак согласования цены сделки путем личных переговоров представителей продавца и покупателя и по переписке</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="NegotiationsComments" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Путем личных переговоров представителей продавца и покупателя (укажите где и кто участвовал)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BuyerGoodsType">
      <xs:annotation>
         <xs:documentation>Покупатель товаров</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="OwnRetailNetwork" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что покупатель товаров реализует товары на внутреннем рынке Российской Федерации через собственную розничную сеть</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SellOne_ThreePersons" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что покупатель товаров продает (планирует продать) товары одному - трем лицам</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SellDifferentPersons" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что покупатель товаров продает (планирует продать) товары разным лицам</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OwnUse" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что покупатель товаров использует товары для собственного потребления или в собственной производственной деятельности</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BuyerSellerType">
      <xs:annotation>
         <xs:documentation>Покупатель/продавец</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Buyer" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что условия поставки были предложены покупателем</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Seller" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что условия поставки были предложены продавцом</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ChoiceGoodsType">
      <xs:annotation>
         <xs:documentation>Выбор товара</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Sample" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак выбора товара по образцу</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Description" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак выбора товара по описанию</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Standard" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак выбора товара в соответствии с действующими стандартами</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CommentsStandard" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>В соответствии с действующими стандартами (указать стандарт, кем принят, регламентирующие его соблюдение документы)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Otherwise" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Иные образом</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="OtherwiseComments" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Иные образом (указать способ)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ChoiceOrderGoodsType">
      <xs:annotation>
         <xs:documentation>Выбор и заказ товара</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="DepartmentAnalysis" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что у покупателя имеется подразделение, занимающееся изучением конъюнктуры рынка и (или) оценкой эффективности сделок</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Analysis" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что подразделение анализировало предложения других продавцов, занимающихся поставками товаров того же класса или вида</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Study" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак возможности предоставить материалы исследований</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DeliveryGoodsUnderContract" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что в рамках контракта поставляются товары одного класса или вида</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ChoiceGood" type="est:ChoiceGoodsType">
            <xs:annotation>
               <xs:documentation>Выбор товара</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OrderGoods" type="est:OrderGoodsType">
            <xs:annotation>
               <xs:documentation>Заказ товара</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DeliveryGoods" type="est:DeliveryGoodsType">
            <xs:annotation>
               <xs:documentation>Поставляемый товар</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ChoiceSellerType">
      <xs:annotation>
         <xs:documentation>Был выбран именно этот продавец, поскольку</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Producer" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что продавец является производителем товаров</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OfficialRepresentativeProducer" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что продавец является официальным представителем производителя</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Guarantee" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что продавец предоставляет гарантийное обслуживание товаров</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ProfitableTerm" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что были предложены более выгодные условия покупки товаров, сравнительно с другими участниками рынка</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CommentsProfitableTerm" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Были предложены более выгодные условия покупки товаров, сравнительно с другими участниками рынка.(указать подробности)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Otherwise" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак выбора продавца по иным причинам</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="OtherwiseComments" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Иные причины (указать)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CostDeliveryType">
      <xs:annotation>
         <xs:documentation>Известна ли Покупателю стоимость доставки.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ExactAmount" type="est:DescriptionCostDeliveryType">
            <xs:annotation>
               <xs:documentation>Известна точная стоимость доставки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NotKnown" type="est:DescriptionCostDeliveryType">
            <xs:annotation>
               <xs:documentation>Не известна стоимость доставки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ApproximateCost" type="est:DescriptionCostDeliveryType">
            <xs:annotation>
               <xs:documentation>Известна примерная стоимость доставки</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DeliveryGoodsType">
      <xs:annotation>
         <xs:documentation>Поставляемый товар</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Retail" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что поставляемый товар подготовлен для розничной продажи</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PackingSorting" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что поставляемый товар требует упаковки, сортировки перед розничной продажей</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ProductionRawMaterial" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что поставляемый товар предназначен для использования в производстве в качестве сырья или сопутствующих товаров</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MeansProduction" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что поставляемый товар будет использован в качестве средства производства</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OwnConsumption" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что поставляемый товар предназначен для собственного потребления</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DescriptionCostDeliveryType">
      <xs:annotation>
         <xs:documentation>Описание стоимость доставки.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Value" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Если значение поля: true или 1 - да; false или 0 - нет</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Quantity" type="clt_ru:AmountType">
            <xs:annotation>
               <xs:documentation>Стоимость доставки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Currency" type="clt_ru:CurrencyA3CodeType">
            <xs:annotation>
               <xs:documentation>Валюта </xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DescriptionKindContractType">
      <xs:annotation>
         <xs:documentation>Описание вида договора</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="DateSettlement" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата заключения</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="ExpiryDate" type="clt_ru:DateCustType">
            <xs:annotation>
               <xs:documentation>Дата окончания действия (если определено договором)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ApplicableLaw" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Применяемое право</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PlaceDetention" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Место заключения</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DescriptionKindCooperationType">
      <xs:annotation>
         <xs:documentation>Описание вида сотрудничества.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="PeriodTime" type="clt_ru:Text40Type">
            <xs:annotation>
               <xs:documentation>Период времени</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Value" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Если значение поля: true или 1 - да; false или 0 - нет</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DescriptionTermsContractType">
      <xs:annotation>
         <xs:documentation>Описание условий договора.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Vaule" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Если значение поля: true или 1 - да; false или 0 - нет</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="AnswerYES" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>В случае ответа ДА  указать подробности</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DiscountType">
      <xs:annotation>
         <xs:documentation>Отличие вызвано скидкой</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="QuantityGoods" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак наличия скидки на количество товаров</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="RepaymentTerms" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак наличия скидки на условия оплаты</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Seasonal" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак наличия сезонной скидки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Other" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак наличия иной скидки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Comments" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Иные (указать вид)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ExcessCustomsCostType">
      <xs:annotation>
         <xs:documentation>Цена товара на внутреннем рынке Российской Федерации превышает заявленную таможенную стоимость, увеличенную на размер таможенных платежей</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Less30percent" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что цена товара на внутреннем рынке РФ превышает заявленную таможенную стоимость, увеличенную на размер таможенных платежей, менее чем на 30%</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="About30_50percent" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что цена товара на внутреннем рынке РФ превышает заявленную таможенную стоимость, увеличенную на размер таможенных платежей, примерно на 30% - 50%</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="About50_100percent" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что цена товара на внутреннем рынке РФ превышает заявленную таможенную стоимость, увеличенную на размер таможенных платежей, примерно на 50% - 100%</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="About3Times" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что цена товара на внутреннем рынке РФ превышает заявленную таможенную стоимость, увеличенную на размер таможенных платежей, примерно в 3 раза</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="About5Times" type="clt_ru:IndicatorType">
            <xs:annotation>
