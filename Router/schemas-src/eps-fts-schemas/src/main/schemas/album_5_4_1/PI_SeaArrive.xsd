               <xs:documentation>Порт и страна прибытия</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DepaturePlace" type="pisa:CountryPortInfoType">
            <xs:annotation>
               <xs:documentation>Порт и страна отправления</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="ShipAgent" type="cat_ru:OrganizationType">
            <xs:annotation>
               <xs:documentation>Сведения о судовом агенте</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="StoresType">
      <xs:annotation>
         <xs:documentation>Сведения о судовых припасах</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="Name" type="clt_ru:NameType">
            <xs:annotation>
               <xs:documentation>Наименование</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Quantity" type="clt_ru:QuantityBasisType">
