         <xs:element name="FundsAvailable" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак необходимости наличных средств</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="GlutWarehouse" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак затоваривания складов</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Bankruptcy" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак банкротства</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Other" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Отличие вызвано иным положением продавца</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Comments" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Иные (указать подробности)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="PricingTransactionsType">
      <xs:annotation>
         <xs:documentation>Формирование цены сделки</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="PriceLeve" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак наличия у покупателя информации об уровне рыночных цен на такого вида товары</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="InitialOfferPriceAppropriateLevel" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что первоначальная цена  предложения корреспондирует такому уровню цен</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="PercentageDeviation" type="clt_ru:CodeVariable3Type">
            <xs:annotation>
               <xs:documentation>Процент отклонения</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SalePriceInitialOfferPrice" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что цена сделки совпадает с первоначальной ценой предложения</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="PercentageDifferences" type="clt_ru:CodeVariable3Type">
            <xs:annotation>
               <xs:documentation>Процент отличия от первоначальной цены.Если значение поля: "true" - да; "false" - нет.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="OtherReasonsDifferences" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Другие причины отличия</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CommentsOtherReasonsDifferences" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Другие причины отличия (указать подробности)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="KeepCorrespondence" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что у покупателя сохранилась переписка</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="GrantCorrespondence" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак возможности представления переписки  таможенным органам</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="ReasonGrantCorrespondence" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Причина, по которой не может быть представлена переписка таможенным органам.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PubliclyOffer" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что первоначальная цена предложения является публичной офертой (в терминах ст. 494 Гражданского кодекса Российской Федерации)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Discount" type="est:DiscountType">
            <xs:annotation>
               <xs:documentation>Отличие вызвано скидкой</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="NonStandardSituationMarket" type="est:NonStandardSituationMarketType">
            <xs:annotation>
               <xs:documentation>Отличие вызвано нестандартной ситуацией на рынке</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="PositionSeller" type="est:PositionSellerType">
            <xs:annotation>
               <xs:documentation>Отличие вызвано положением Продавца</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="SpecialRelationship" type="est:SpecialRelationshipType">
            <xs:annotation>
               <xs:documentation>Отличие вызвано особыми отношениями Продавца и покупателя</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="AlignmentPrices" type="est:AlignmentPricesType">
            <xs:annotation>
               <xs:documentation>Согласование цены сделки.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="ExpressPubliclyOffer" type="est:ExpressPubliclyOfferType">
            <xs:annotation>
               <xs:documentation>Каким образом была выражена публичная оферта Продавца.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ValueFormingPrice" type="est:ValueFormingPriceType">
            <xs:annotation>
               <xs:documentation>Известно ли покупателю, исходя из какой величины формировалась цена первоначального предложения.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="PropertyType">
      <xs:annotation>
         <xs:documentation>Когда осуществляется переход права собственности от Продавца к Покупателю</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="DayShipping" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак перехода права собственности от продавца к покупателю на день отгрузки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DayPayment" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак перехода права собственности от продавца к покупателю на день оплаты</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OtherDay" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак перехода права собственности от продавца к покупателю в иной день</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CommentsOtherDay" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Иной день(подробности)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="RiskLossDamageType">
      <xs:annotation>
         <xs:documentation>Когда осуществляется переход риска утраты/порчи товара на покупателя</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Warehouse" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак перехода риска утраты/порчи товара на покупателя на складе отправителя</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Disposition" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак перехода риска утраты/порчи товара на покупателя после передачи лицу, осуществляющему</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ProcessDelivery" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак перехода риска утраты/порчи товара на покупателя в процессе доставки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CommentsProcessDelivery" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>В процессе доставки (подробности)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PlaceDelivery" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак перехода риска утраты/порчи товара на покупателя в месте доставки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OtherPlace" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак перехода риска утраты/порчи товара на покупателя в ином месте</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="Address" type="cat_ru:AddressType">
            <xs:annotation>
               <xs:documentation>Адрес </xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="SaleGoodsDomesticMarketType">
      <xs:annotation>
         <xs:documentation>Реализация товаров на внутреннем рынке</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element minOccurs="0" name="FinalSellingPrice" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что цена реализации товара известна конечному потребителю на внутреннем рынке</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="UnitSellingPriceGoods" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Укажите цену реализации единицы товара на внутреннем рынке (в т.ч. планируемую)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Cost" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Укажите общий размер расходов Покупателя при реализации товаров на внутреннем рынке,которые должны быть включены в цену реализации для их покрытия</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Profitability" type="clt_ru:CodeVariable3Type">
            <xs:annotation>
               <xs:documentation>Укажите процент планируемой рентабельности сделки</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="BuyerGoods" type="est:BuyerGoodsType">
            <xs:annotation>
               <xs:documentation>Покупатель товаров</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ExcessCustomsCost" type="est:ExcessCustomsCostType">
            <xs:annotation>
               <xs:documentation>Цена товара на внутреннем рынке Российской Федерации превышает заявленную таможенную стоимость, увеличенную на размер таможенных платежей</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MarketPrice" type="est:MarketPriceType">
            <xs:annotation>
               <xs:documentation>Известна ли Покупателю рыночная цена в Российской Федерации товаров того же класса или вида при продаже их конечному потребителю.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="SearchSellerType">
      <xs:annotation>
         <xs:documentation>Поиск продавца для покупки данного товара или ранее до начала сотрудничества осуществлялся</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Middleman" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак участия посредника</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Commercial" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак использования рекламных объявлений</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Exchange" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак поиска продавца на бирже</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Otherwise" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак поиска продавца иным образом</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="CommentsOtherwise" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Иным образом (указать способ)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="SpecialRelationshipType">
      <xs:annotation>
         <xs:documentation>Отличие вызвано особыми отношениями Продавца и покупателя</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Cooperation" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак наличия длительной истории сотрудничества</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Prepayment" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что покупатель предоставляет продавцу предоплату</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ExtraDuty" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Покупатель принимает дополнительные обязательства по объему продажи товаров</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Other" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Отличие вызвано иными особыми отношениями продавца и покупателя</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Comments" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Иные (указать подробности)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="TermsContractType">
      <xs:annotation>
         <xs:documentation>Условия контракта</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ProvidedDependPurchase" type="est:DescriptionTermsContractType">
            <xs:annotation>
               <xs:documentation>Условие, при котором продажа одного наименования товара зависит от покупки товара другого наименования</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DeliveryGoodsBuyer" type="est:DescriptionTermsContractType">
            <xs:annotation>
               <xs:documentation>Поставку товаров со стороны Покупателя в пользу Продавца бесплатно или по сниженной цене</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CounterDelivery" type="est:DescriptionTermsContractType">
            <xs:annotation>
               <xs:documentation>Встречную поставку товаров покупателем продавцу.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="TransportInsuranceType">
      <xs:annotation>
         <xs:documentation>Заключается ли договор транспортного страхования в отношении товаров, поставляемых по контракту</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Not" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что договор транспортного страхования в отношении товаров, поставляемых по контракту, не заключается</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Risk" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что заключался договор транспортного страхования в отношении товаров, поставляемых по контракту, с ответственностью за все риски</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SimpleAverage" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что заключался договор транспортного страхования в отношении товаров, поставляемых по контракту, с ответственностью за частную аварию</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Damage" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что заключался договор транспортного страхования в отношении товаров, поставляемых по контракту, без ответственности за повреждения, кроме случаев крушения</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OtherTerms" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что заключался договор транспортного страхования в отношении товаров, поставляемых по контракту, на иных условиях</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="DescriptionOtherTerms" type="clt_ru:FreeText250Type">
            <xs:annotation>
               <xs:documentation>Указание подробности других условий.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ValueFormingPriceType">
      <xs:annotation>
         <xs:documentation>Известно ли покупателю, исходя из какой величины формировалась цена первоначального предложения.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ExchangePrice" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак формирования цены первоначального предложения, исходя из биржевой цены</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PriceOtherProposals" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак формирования цены первоначального предложения, исходя из цены на товары того же класса или вида других предложений при продаже товаров на экспорт из страны производства</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PriceOtherVendors" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак формирования цены первоначального предложения, исходя из цен на товары того же класса или вида у других продавцов при продаже товаров на экспорт из страны отправления</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CostPrice" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак формирования цены первоначального предложения, исходя из себестоимости производства</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SalesDomesticMarket" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак формирования цены первоначального предложения, исходя из цены возможной реализации на внутреннем рынке Российской Федерации</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NotKnown" type="clt_ru:IndicatorType">
            <xs:annotation>
               <xs:documentation>Признак того, что способ формирования цены первоначального предложения неизвестен</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
</xs:schema>
