<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:ainin="urn:customs.ru:Information:CustomsDocuments:ActInspectionIn:5.4.0" xmlns:catESAD_cu="urn:customs.ru:CUESADCommonAggregateTypesCust:5.4.0" xmlns:catESAD_ru="urn:customs.ru:RUCommonAggregateTypes:5.4.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.4.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:customs.ru:Information:CustomsDocuments:ActInspectionIn:5.4.0" elementFormDefault="qualified" version="5.4.0.0">
	<xs:import namespace="urn:customs.ru:RUCommonAggregateTypes:5.4.0" schemaLocation="RUESADCommonAggregateTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.4.0" schemaLocation="CommonAggregateTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CommonLeafTypes:5.4.0" schemaLocation="CommonLeafTypesCust.xsd"/>
	<xs:import namespace="urn:customs.ru:CUESADCommonAggregateTypesCust:5.4.0" schemaLocation="CUESADCommonAggregateTypesCust.xsd"/>
	<xs:annotation>
		<xs:documentation>Акт таможенного досмотра (внутренний формат)</xs:documentation>
	</xs:annotation>
	<xs:element name="ActInspectionIn" type="ainin:ActInspectionInType"/>
	<xs:complexType name="ActInspectionInType">
		<xs:annotation>
			<xs:documentation>Акт таможенного досмотра (внутренний формат)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:BaseDocType">
				<xs:sequence>
					<xs:element name="BeginInspectionTime" type="clt_ru:TimeCustType">
						<xs:annotation>
							<xs:documentation>Время начала таможенного досмотра (осмотра)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BeginInspectionDate" type="clt_ru:DateCustType">
						<xs:annotation>
							<xs:documentation>Дата начала таможенного досмотра (осмотра)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndInspectionTime" type="clt_ru:TimeCustType">
						<xs:annotation>
							<xs:documentation>Время окончания таможенного досмотра (осмотра)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndInspectionDate" type="clt_ru:DateCustType">
						<xs:annotation>
							<xs:documentation>Дата окончания таможенного досмотра (осмотра)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ActType" type="clt_ru:LetterIndicatorType">
						<xs:annotation>
							<xs:documentation>Признак досмотра: 1 - досмотре всех товаров, заявленных в ДТ, 2 - досмотр части товаров, заявленных в ДТ, 3 - досмотр товаров, заявленных в нескольких ДТ</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ATDGlobalID" type="clt_ru:DocumentIDType">
						<xs:annotation>
							<xs:documentation>Уникальный идентификатор акта таможенного досмотра</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ATDKind" type="clt_ru:LetterIndicatorType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Признак досмотра: 0 - досмотр; 1 - осмотр</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ModificationDate" type="clt_ru:DateCustType">
						<xs:annotation>
							<xs:documentation>Дата модификации </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ModificationTime" type="clt_ru:TimeCustType">
						<xs:annotation>
							<xs:documentation>Время модификации</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TechMarkCode" type="clt_ru:Code3Type">
						<xs:annotation>
							<xs:documentation>Код  проставленной технологической отметки. Код технологической операции заполняется:  001- ошибочно переданный АТД ; 002 - АТД, содержащий ошибку в регистрационном номере; 000 - в прочие случаи.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CustomsMark" type="ainin:FreeText4000" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Служебные отметки </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ShortFormSign" type="clt_ru:IndicatorType">
						<xs:annotation>
							<xs:documentation>Признак составления АТДО в сокращенной форме: 1 - да; 0 - нет</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndComposeDate" type="clt_ru:DateCustType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Дата завершения составления АТДО</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndComposeTime" type="clt_ru:TimeCustType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Время завершения составления АТДО</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ATDRegNumber" type="cat_ru:GTDID6Type">
						<xs:annotation>
							<xs:documentation>Регистрационный номер акта таможенного досмотра</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InstructionNumber" type="ainin:InspInstructionType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Сведения  о поручении на досмотр</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InspectionPlace" type="ainin:InspectionPlaceType">
						<xs:annotation>
							<xs:documentation>Описание места проведения досмотра. Иная информация о месте досмотра</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InspPerson" type="ainin:InspPersonType">
						<xs:annotation>
							<xs:documentation>Присутствующие лица</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InspTransport" type="ainin:InspTransportType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Сведения о транспортных  средствах</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InspectionInfo" type="ainin:InspectionInfoType">
						<xs:annotation>
							<xs:documentation>Информация о проведенном досмотре</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ATDTechOperation" type="ainin:TechOperationType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Информация о технологической операции</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1006036E">
					<xs:annotation>
						<xs:documentation>Идентификатор вида документа</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>Локальные прикладные типы.</xs:documentation>
	</xs:annotation>
	<xs:complexType name="ContainerType">
		<xs:annotation>
			<xs:documentation>Сведения о контейнерах.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ContainerQuantity" type="clt_ru:Quantity8Type">
				<xs:annotation>
					<xs:documentation>Количество контейнеров. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContainerIdentificator" type="clt_ru:ContainerIdentityType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Номер (идентификатор) контейнера.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FactWeightInfoType">
		<xs:annotation>
			<xs:documentation>Сведения об общем фактическом весе товаров</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="WeightNettoFact" type="clt_ru:QuantityBasisType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Фактический вес товара нетто, кг</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightBruttoFact" type="clt_ru:QuantityBasisType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Фактический вес товара брутто, кг</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightMethodKind" type="clt_ru:LetterIndicatorType">
				<xs:annotation>
					<xs:documentation>Способ определения фактического веса товаров: "1" - фактическое взвешивание,  "2" - определение среднего веса одного места взвешиванием нескольких мест, "3" - расчетный метод, "4" - прочее.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightMetodDesc" type="ainin:FreeText4000" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Примечание к способу взвешивания: количество взвешенных мест, способов расчетов, описание прочего способа взвешивания.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IdentifyMeanType">
		<xs:annotation>
			<xs:documentation>Cредство идентификации</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IdentMeanState" type="clt_ru:LetterIndicatorType">
				<xs:annotation>
					<xs:documentation>Статус средства идентификации 1 - средство идентификации до начала проведения таможенного досмотра 2 - средство идентификации после проведения таможенного досмотра (наложенные)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentNumber" type="clt_ru:Text50Type">
				<xs:annotation>
					<xs:documentation>Номер наложенного средства идентификации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentBreakFlag" type="clt_ru:IndicatorType">
				<xs:annotation>
					<xs:documentation>Признак нарушения средств идентификации ("0" - не нарушено, "1" - нарушено)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentKind" type="clt_ru:LetterIndicatorType">
				<xs:annotation>
					<xs:documentation>Вид средств идентификации: 1 - пломба, 2 - печать, 3 - иное</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentDesc" type="ainin:FreeText4000" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Примечание по средству идентификации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecordNumber" type="clt_ru:Count5Type">
				<xs:annotation>
					<xs:documentation>Порядковый номер записи в таблице БД</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IdentityMeansInfoType">
		<xs:annotation>
			<xs:documentation>Средства идентификации</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IdentBeforeNum" type="clt_ru:Quantity4Type">
				<xs:annotation>
					<xs:documentation>Кол-во средств идентификации до начала проведения досмотра (0 - без средств идентификации)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentCrashFlag" type="clt_ru:IndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак нарушения средств идентификации ("0" - не нарушены, "1" - нарушены)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentEndNum" type="clt_ru:Quantity4Type">
				<xs:annotation>
					<xs:documentation>Кол-во наложенных средств идентификации после проведения таможенного досмотра (0 - без средств идентификации)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentMeansDesc" type="ainin:FreeText4000" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Общие примечания по средствам идентификации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentifyMean" type="ainin:IdentifyMeanType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Cредство идентификации</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InspGTDType">
		<xs:annotation>
			<xs:documentation>Номер ДТ в которой заявлены сведения о досматриваемых  товарах</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:GTDIDType">
				<xs:sequence>
					<xs:element name="RecordNumber" type="clt_ru:Count5Type">
						<xs:annotation>
							<xs:documentation>Порядковый номер записи в таблице БД</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InspGoodsType">
		<xs:annotation>
			<xs:documentation>АТД - сведения о товарe</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:GoodsBaseType">
				<xs:sequence>
					<xs:element name="CountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Двузначный буквенный альфа-2 код страны изготовления (производства) товара по общероссийскому классификатору стран мира (согласно маркировке)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CountryName" type="clt_ru:CountryNameType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Краткое наименование страны изготовления (производства) товара по общероссийскому классификатору стран мира (согласно маркировке)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CountrySrcFlag" type="clt_ru:LetterIndicatorType">
						<xs:annotation>
							<xs:documentation>Код источника информации о стране изготовления: 1 - маркировка на товаре 2- маркировка на упаковке  3 -сведения из документов 4- другие источники</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CountrySrcDesc" type="ainin:FreeText4000" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Описание источника информации о стране изготовления. Заполняется  при CountrySrcFlag =  4- другие источники</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DocBruttoWeight" type="clt_ru:QuantityBasisType">
						<xs:annotation>
							<xs:documentation>Вес брутто товаров по документам (кг.)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GoodsDamageFlag" type="clt_ru:IndicatorType">
						<xs:annotation>
							<xs:documentation>Признак повреждения товара 0 -  повреждения отсутствуют; 1 - товар поврежден</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DamageDesc" type="ainin:FreeText4000" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Описание повреждения товаров и упаковки</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GoodsViolationFlag" type="clt_ru:LetterIndicatorType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Признак выявленных несоответствий относительно имеющихся у инспектора документов: 1 - по весу нетто; 2 -по количеству товаров, в единицах измерения, отличных от кг; 3 - по описанию; 4 - по иным признакам</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GoodsViolationDesc" type="ainin:FreeText4000" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Описание иных  признаков несоответствия. Заполняется при GoodsViolationFlag = 4 - по иным признакам</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GoodsTestFlag" type="clt_ru:LetterIndicatorType">
						<xs:annotation>
							<xs:documentation>Признак взятия проб и образцов: 0 - пробы и образцы товара не отбирались; 1- пробы и образцы товара отбирались</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RecordNumber" type="clt_ru:Count5Type">
						<xs:annotation>
							<xs:documentation>Порядковый номер записи в таблице БД</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GoodsNumericDT" type="clt_ru:Count3Type" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Номер товара по ДТ</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PackageInfo" type="ainin:InspPackageType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>АТД - сведения об упаковках</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SupplementaryQuantity" type="cat_ru:SupplementaryQuantityType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Количество товаров в  единице измерения, отличной от кг</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CargoInfo" type="ainin:InspPackageType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Сведения о виде груза</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PlaceInfo" type="ainin:InspPackageType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Сведения о таре (потребительской / индивидуальной)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PalleteInfo" type="ainin:InspPackageType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Сведения о поддонах / палетах</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InspInstructionType">
		<xs:annotation>
			<xs:documentation>Сведения о поручении на досмотр</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Time" type="clt_ru:TimeCustType">
				<xs:annotation>
					<xs:documentation>Время выдачи поручения на досмотр</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomsCode" type="clt_ru:CustomsCodeType">
				<xs:annotation>
					<xs:documentation>Код таможенного органа, зарегистрировавшего ПТД.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RegistrationDate" type="clt_ru:DateCustType">
				<xs:annotation>
					<xs:documentation>Дата регистрации ПТД.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PTDNumber" type="clt_ru:Code6Type">
				<xs:annotation>
					<xs:documentation>Порядковый номер ПТД   по журналу регистрации.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InspPackageType">
		<xs:annotation>
			<xs:documentation>АТД - сведения об упаковках</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="catESAD_cu:PackingInformationType">
				<xs:sequence>
					<xs:element name="PackageDescription" type="ainin:FreeText4000" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Описание упаковки и расфасовки товара</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PackDestructFlag" type="clt_ru:LetterIndicatorType">
						<xs:annotation>
							<xs:documentation>Признак наличия повреждений товаров и упаковки 0 - повреждений нет 1 - повреждения есть</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PackDestructDesc" type="ainin:FreeText4000" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Описание повреждений упаковки</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RecordNumber" type="clt_ru:Count5Type">
						<xs:annotation>
							<xs:documentation>Порядковый номер записи в таблице БД</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InspPersonType">
		<xs:annotation>
			<xs:documentation>Присутствующие лица</xs:documentation>
		</xs:annotation>
		<xs:sequence>
